{
  "expo": {
    "name": "Boocozmo",
    "slug": "boocozmo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.boocozmo.boocozmo",
      "enableEdgeToEdge": true,
      "config": {
        "googleMaps": {
          "apiKey": null
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "db7cc7dc-6136-4295-b683-08a49807c5f6"
      },
      "mapTilerApiKey": "pps3Llejghq7tBKaMmgf"
    }
  }
}